r\n.container {\r\n  padding: 8rem 0;\r\n  max-width: 100%;\r\n}\r\n\r\n/** FORMULA */\r\n\r\n.formula-container {\r\n  margin-top: 4rem;\r\n}\r\n\r\n.formula__box:not(:last-child) {\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.formula__btn {\r\n  display: flex;\r\n  width: 100%;\r\n  background-color: var(--color-black);\r\n  height: 6rem;\r\n  padding: 0 3.5rem;\r\n  align-items: center;\r\n  border: none;\r\n  cursor: pointer;\r\n  color: var(--color-white);\r\n  font-size: 2rem;\r\n  font-weight: 700;\r\n  text-transform: uppercase;\r\n  transition: all 0.2s ease;\r\n}\r\n\r\n.formula__btn:hover {\r\n  transform: scale(1.05);\r\n  background-color: var(--color-primary);\r\n}\r\n\r\n.formula__btn--active {\r\n  transform: scale(1.05);\r\n  background-color: var(--color-primary);\r\n}\r\n\r\n.formula__content {\r\n  display: none;\r\n  visibility: hidden;\r\n  margin-top: 1rem;\r\n}\r\n\r\n.formula__content--active {\r\n  display: flex;\r\n  visibility: visible;\r\n  transform: scale(1.05);\r\n  padding: 2rem 2.5rem;\r\n  background-color: var(--color-white);\r\n  backface-visibility: hidden;\r\n}\r\n\r\n.formula__form {\r\n  transform: scale(0.95);\r\n}\r\n\r\n.formula__input-container {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.formula__input-container {\r\n  margin-bottom: 2rem;\r\n}\r\n\r\n.formula__label {\r\n  margin-right: 2rem;\r\n  width: 14rem;\r\n  color: #666;\r\n  font-weight: 700;\r\n  position: relative;\r\n}\r\n\r\n.formula__label::after {\r\n  content: '=';\r\n  font-size: 1.6rem;\r\n  position: absolute;\r\n  right: 0;\r\n}\r\n\r\n.formula__input {\r\n  border: 0.2rem solid transparent;\r\n  background-color: var(--color-gray-dark);\r\n  padding: 1rem 1.4rem;\r\n  border-radius: 0.4rem;\r\n  font-size: 1.4rem;\r\n}\r\n\r\n.formula__input:focus {\r\n  border: 0.2rem solid var(--color-primary-light);\r\n}\r\n\r\n.formula__img-container {\r\n  display: flex;\r\n  justify-content: flex-end;\r\n  width: 100%;\r\n}\r\n\r\n.formula__img {\r\n  display: block;\r\n  height: 25rem;\r\n}\r\n\r\n.formula__form-btn {\r\n  padding: 1rem;\r\n  border: none;\r\n  background-color: var(--color-primary-light);\r\n  border-radius: 0.4rem;\r\n  cursor: pointer;\r\n  width: 100%;\r\n  font-weight: 700;\r\n  font-size: 1.6rem;\r\n  color: var(--color-black);\r\n  transition: background-color 0.2s ease;\r\n}\r\n\r\n.formula__form-btn:hover {\r\n  background-color: var(--color-primary);\r\n}\r\n\r\n.formula__info {\r\n  width: 100%;\r\n  border: 0.2rem solid var(--color-warning);\r\n  color: var(--color-black);\r\n  font-weight: 700;\r\n  padding: 1rem 2rem;\r\n  margin-bottom: 2rem;\r\n  border-radius: 0.4rem;\r\n}\r\n\r\n.formula__results {\r\n  transform: scale(0.95);\r\n}\r\n\r\n.formula__result {\r\n  display: flex;\r\n  align-items: center;\r\n  height: 4.4rem;\r\n  margin-bottom: 2rem;\r\n}\r\n"},"lineCount":232}},"error":null,"hash":"0fa2aea1b6e1d9ad564ea94e6797de13","cacheData":{"env":{}}}